{
  "realm": "bank",
  "enabled": true,
  "accessTokenLifespan": 300,
  "ssoSessionIdleTimeout": 1800,
  "registrationAllowed": true,
  "registrationEmailAsUsername": false,
  "editUsernameAllowed": true,
  "verifyEmail": false,
  "resetPasswordAllowed": true,
  "rememberMe": true,
  "loginTheme": "bank-theme",
  "defaultRoles": [ "USER" ],
  "roles": {
    "realm": [
      { "name": "USER", "description": "Пользователь банка" },
      { "name": "ADMIN", "description": "Администратор" },
      { "name": "ACCOUNT_ACCESS", "description": "Доступ к сервису аккаунтов" },
      { "name": "CASH_ACCESS", "description": "Доступ к сервису наличных" },
      { "name": "TRANSFER_ACCESS", "description": "Доступ к сервису переводов" },
      { "name": "NOTIFICATION_ACCESS", "description": "Доступ к сервису уведомлений" }
    ]
  },
  "clients": [
    {
      "clientId": "front-ui",
      "name": "Bank Frontend",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "90bd600c-7781-4435-961e-1f5139265f7c",
      "baseUrl": "http://localhost:8081",
      "redirectUris": [
        "http://localhost:8081/*",
        "http://localhost:8081/login/oauth2/code/keycloak"
      ],
      "webOrigins": [
        "+"
      ],
      "protocol": "openid-connect",
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": true,
      "publicClient": false,
      "frontchannelLogout": true,
      "attributes": {
        "post.logout.redirect.uris": "http://localhost:8081/*",
        "display.on.consent.screen": "false",
        "frontchannel.logout.url": "http://localhost:8081/logout",
        "frontchannel.logout.session.required": "true",
        "backchannel.logout.session.required": "true",
        "backchannel.logout.revoke.offline.tokens": "false"
      },
      "protocolMappers": [
        {
          "name": "realm roles",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-realm-role-mapper",
          "config": {
            "multivalued": "true",
            "userinfo.token.claim": "true",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "realm_access.roles",
            "jsonType.label": "String"
          }
        }
      ],
      "serviceAccountsEnabled": true,
      "fullScopeAllowed": true
    },
    {
